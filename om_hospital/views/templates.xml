<?xml version="1.0" encoding="utf-8"?>


<odoo>

    <template id="patients_page" name="Hospital Patients">
        <t t-call="website.layout">
            <div class="container">
                Hey Mohsen and Mes3edaa
                <table class="table-bordered">
                    <thead>
                        <tr>
                            <td>
                                Name
                            </td>
                            <td>
                                Age
                            </td>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="patients" t-as="o">
                            <tr>
                                <td>
                                    <t t-esc="o.patient_name"/>
                                </td>
                                <td>
                                    <t t-esc="o.patient_age"/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </div>
        </t>
    </template>

</odoo>